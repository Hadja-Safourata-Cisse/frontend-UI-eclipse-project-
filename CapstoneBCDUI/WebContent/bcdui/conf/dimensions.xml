<?xml version="1.0" encoding="UTF-8"?>
<Dimensions
		xmlns="http://www.businesscode.de/schema/bcdui/dimmeas-1.0.0">
		<Dimension id="myDimension">
				<Hierarchy bindingSet="myCities">
						<Level id="country" caption="Country" bRef="country" />
						<Level id="city" caption="City"   bRef="city" />
				</Hierarchy>
		</Dimension>
		<Dimension id="DimOriginCity">
				<Hierarchy bindingSet="myBooking">
						<Level id="originCity" caption="originCity"
								bRef="originCity" />
						<Level id="id" caption="id" bRef="id" />
				</Hierarchy>
		</Dimension>
		<Dimension id="DimDestinationCity">
				<Hierarchy bindingSet="myBooking">
						<Level id="destinationCity" caption="destinationCity"
								bRef="destinationCity" />
						<Level id="id" caption="id" bRef="id" />
				</Hierarchy>
		</Dimension>

</Dimensions>




















